{
  "name": "generi",
  "version": "0.2.0",
  "description": "A Simple CHANGELOG.md Generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/Novout/generi"
  },
  "bugs": {
    "url": "https://github.com/Novout/generi/issues"
  },
  "homepage": "https://github.com/Novout/generi/",
  "author": "novout",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "build/index.js",
  "files": [
    "build/**/*",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "changelog",
    "git",
    "publish",
    "monorepo",
    "lerna",
    "cli"
  ],
  "scripts": {
    "dev": "rimraf ./.git && rimraf ./generi.json && yarn start",
    "start": "yarn build && node build/index.js",
    "build": "rimraf ./build && tsc -d",
    "test": "jest --config ./jest.config.json",
    "set": "prettier --write src/",
    "pub": "yarn test && yarn build && npm publish"
  },
  "bin": {
    "generi": "build/index.js"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "gradient-string": "^2.0.0",
    "picocolors": "^1.0.0",
    "sade": "^1.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/gradient-string": "^1.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.1",
    "@types/sade": "^1.7.4",
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}